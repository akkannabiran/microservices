{
  "name": "jdbc-sink-connector",
  "config": {
    "batch.size": 3,
    "connector.class": "com.carefirst.nexus.connect.jdbc.CustomJDBCSinkConnector",
    "database.catalog": "mydb",
    "database.host": "127.0.0.1",
    "database.password": "pgadmin",
    "database.port": 5432,
    "database.schema": "poc",
    "database.username": "postgres",
    "errors.deadletterqueue.context.headers.enable": true,
    "errors.deadletterqueue.topic.name": "dlt_topic",
    "errors.deadletterqueue.topic.replication.factor": 1,
    "errors.log.enable": true,
    "errors.log.include.messages": true,
    "errors.retry.delay.max.ms": 60000,
    "errors.retry.timeout": 0,
    "errors.tolerance": "none",
    "key.converter": "org.apache.kafka.connect.json.JsonConverter",
    "key.converter.schemas.enable": false,
    "login.timeout.seconds": 15,
    "max.retries": 5,
    "producer.bootstrap.servers": "localhost:9092",
    "producer.key.serializer": "org.apache.kafka.common.serialization.StringSerializer",
    "producer.value.serializer": "com.carefirst.nexus.connect.jdbc.serialization.JsonSerialization",
    "retry.backoff.ms": 10000,
    "tables": "subgroup",
    "tables.subgroup.columns.mapping": "subgroup_json=$.subgroup_json_val",
    "tables.subgroup.mode": "insert",
    "tables.subgroup.mode.insert.exclude.columns": "subgroup_id",
    "tables.subgroup.mode.update.clause": "subgroup.aud_insrt_id = @aud_insrt_id",
    "tables.subgroup.mode.update.columns": "aud_updt_tmstp=aud_insrt_tmstp, subgroup_json",
    "tables.subgroup.mode.update.keys": "sub_group_id, srce_sys_cd, group_id",
    "tables.subgroup.payload": "$",
    "tasks.max": 1,
    "topics": "sink_topic",
    "transforms": "timestamp",
    "transforms.timestamp.field": "mydate",
    "transforms.timestamp.format": "yyyy-MM-dd",
    "transforms.timestamp.target.type": "Date",
    "transforms.timestamp.type": "org.apache.kafka.connect.transforms.TimestampConverter$Value",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": false
  }
}